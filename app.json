{
  "expo": {
    "name": "Small Talk",
    "slug": "small-talk",
    "owner": "Nick Wylynko",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f95858"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 500
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smalltalk.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.36436457019-3u4lau2k5m2a971p54fgatuqo1b5icvi"
        }
      }
    },
    "android": {
        "package": "com.smalltalk.app",
        "googleServicesFile": "./google-services.json"
    }
  }
}