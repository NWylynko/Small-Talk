{
  "expo": {
    "name": "Small Talk",
    "slug": "small-talk",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smalltalk.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "####"
        }
      }
    },
    "android": {
        "package": "com.smalltalk.app",
        "googleServicesFile": "./google-services.json"
    }
  }
}